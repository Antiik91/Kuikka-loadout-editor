<Window x:Class="KuikkaLoadoutEditor.CreateRole"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CreateRole" Height="409" Width="453">
    <Grid>
        <Canvas x:Name="Canvas_CreateRole" HorizontalAlignment="Left" Height="484" Margin="-150,-77,-191,-28" VerticalAlignment="Top" Width="786"/>
        <TextBox x:Name="arsenalLoadout" HorizontalAlignment="Left" Height="261" TextWrapping="Wrap" AcceptsReturn="True" VerticalAlignment="Top" Width="311" Margin="10,72,0,0" TextChanged="arsenalLoadout_TextChanged" UndoLimit="0"/>
        <Button x:Name="newLoadout" Content="Lisää loadoutiin" HorizontalAlignment="Left" Margin="48,347,0,0" VerticalAlignment="Top" Width="106" Click="newLoadout_Click" RenderTransformOrigin="0.509,-1.455"/>
        <Label Content="Liitä arsenal koodisi alle" HorizontalAlignment="Left" Margin="19,41,0,0" VerticalAlignment="Top" Width="135"/>
        <TextBox x:Name="role" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Margin="64,13,0,0" TextChanged="role_TextChanged"/>
        <Label Content="Rooli:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="19,13,0,0"/>
        <Label Content="Liitä arsenal koodisi alle" HorizontalAlignment="Left" Margin="19,41,0,0" VerticalAlignment="Top" Width="135"/>

    </Grid>
</Window>
